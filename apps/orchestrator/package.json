{
  "name": "@dakkah/orchestrator",
  "version": "1.0.0",
  "description": "Dakkah CityOS Payload CMS Orchestrator",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev -p 3001",
    "build": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next build",
    "start": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next start -p 3001",
    "lint": "next lint",
    "payload": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" payload",
    "generate:types": "payload generate:types",
    "migrate": "payload migrate",
    "migrate:create": "payload migrate:create",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.0.0-beta.111",
    "@payloadcms/next": "^3.0.0-beta.111",
    "@payloadcms/plugin-cloud": "^3.0.0-beta.111",
    "@payloadcms/richtext-lexical": "^3.0.0-beta.111",
    "@payloadcms/storage-s3": "^3.0.0-beta.111",
    "@payloadcms/ui": "^3.0.0-beta.111",
    "@types/bull": "^4.10.4",
    "bull": "^4.16.5",
    "crypto": "npm:node-stdlib-browser@^1.2.0",
    "graphql": "^16.8.1",
    "ioredis": "^5.9.1",
    "jose": "^5.2.0",
    "next": "15.0.8",
    "payload": "^3.0.0-beta.111",
    "react": "^19.0.0-rc.0",
    "react-dom": "^19.0.0-rc.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/react": "npm:types-react@19.0.0-rc.0",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.0",
    "@vitest/ui": "^4.0.16",
    "cross-env": "^7.0.3",
    "typescript": "5.6.2",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
